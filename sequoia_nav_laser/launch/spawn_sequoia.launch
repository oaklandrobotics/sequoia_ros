<?xml version="1.0"?>
<launch>

  <arg name="robot_type" />
  <arg name="robot_name" default="$(arg robot_type)" />
  <arg name="description_pkg" />
  <arg name="start_x" default="0.0" />
  <arg name="start_y" default="0.0" />
  <arg name="start_z" default="0.0" />
  <arg name="start_yaw" default="0.0" />
  <arg name="gps" default="0" />
  <arg name="gps_rate" default="1.0" />
  <arg name="pub_tf" default="0" />
  <arg name="pub_odom" default="0" />
  <arg name="pub_heading" default="0" />
  <arg name="use_sensors" default="0" />
  <arg name="tf_freq" default="50.0" />

 <!-- Gazebo model spawner -->
  

  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find sequoia_description)/urdf/sequoia.urdf.xacro'" />
  <node pkg="gazebo_ros" type="spawn_model"  name="spawn_sequoia" args="-urdf -param robot_description -model vehicle -x $(arg start_x) -y $(arg start_y) -z $(arg start_z) -Y $(arg start_yaw)"/>
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="state_publisher">
    <param name="publish_frequency" value="100.0" />
  </node>

</launch>

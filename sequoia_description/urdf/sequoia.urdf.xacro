<?xml version="1.0"?>

<robot name="sequoia" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find sequoia_description)/urdf/sequoia_gazebo.urdf.xacro"/>
  <xacro:include filename="$(find sequoia_description)/urdf/sequoia_sensors.urdf.xacro"/>

  <link name="base_footprint">
  </link>
  <link name="rearaxle">
  </link>
  <link name="frontaxle">
  </link>

  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="900.0"/>
      <inertia ixx="1000.0" ixy="0" ixz="0" iyy="1000.0" iyz="0" izz="1000.0"/>
    </inertial>
    <visual name="base_link_visual">
      <origin xyz="-0.0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="2.3241 1.3516 0.635"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision name="base_link_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="2.3241 1.3516 0.635"/>
      </geometry>
    </collision>
  </link>

  <link name="front_end">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="100.0"/>
      <inertia ixx="1000.0" ixy="0" ixz="0" iyy="1000.0" iyz="0" izz="1000.0"/>
    </inertial>
    <visual name="front_end_visual">
      <origin xyz="-0.0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.3175" length="1.3516"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision name="front_end_collision">
      <origin xyz="-0.0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.3175" length="1.3516"/>
      </geometry>
    </collision>
  </link>

  <link name="trunk">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="20.0"/>
      <inertia ixx="1000.0" ixy="0" ixz="0" iyy="1000.0" iyz="0" izz="1000.0"/>
    </inertial>
    <visual name="trunk_visual">
      <origin xyz="-0.0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 1.2 0.4"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision name="trunk_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 1.2 0.4"/>
      </geometry>
    </collision>
  </link>

  <link name="dome">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="200.0"/>
      <inertia ixx="1000.0" ixy="0" ixz="0" iyy="1000.0" iyz="0" izz="1000.0"/>
    </inertial>
    <visual name="dome_visual">
      <origin xyz="-0.0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.8"  length="1.350"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision name="dome_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.8"  length="1.350"/>
      </geometry>
    </collision>
  </link>

  <link name="dome_small">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1000.0" ixy="0" ixz="0" iyy="1000.0" iyz="0" izz="1000.0"/>
    </inertial>
    <visual name="dome_small_visual">
      <origin xyz="-0.0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.2"  length="1.345"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision name="dome_small_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.2"  length="1.345"/>
      </geometry>
    </collision>
  </link>

  <link name="left_steer">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="0.37" ixy="0" ixz="0" iyy="0.4" iyz="0" izz="0.6"/>
    </inertial>
  </link>

  <link name="right_steer">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="0.37" ixy="0" ixz="0" iyy="0.4" iyz="0" izz="0.6"/>
    </inertial>
  </link>

  <link name="wheel_fl">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.75"/>
      <inertia ixx="0.305" ixy="0" ixz="0" iyy="0.305" iyz="0" izz="0.5445"/>
    </inertial>
    <visual name="wheel_fl_visual">
      <origin xyz="-0.0 0 0" rpy="-3.141593 0 0"/>
      <geometry>
        <cylinder radius="0.254" length="0.127"/>
      </geometry>
      <material name="Tire_001.004"/>
    </visual>
    <collision name="wheel_fl_collision">
      <origin xyz="-0.0 0 0" rpy="-3.141593 0 0"/>
      <geometry>
        <cylinder radius="0.254" length="0.127"/>
      </geometry>
    </collision>
  </link>

  <link name="wheel_fr">
    <inertial>
      <origin xyz="0.0 0 0" rpy="0 0 0"/>
      <mass value="0.75"/>
      <inertia ixx="0.305" ixy="0" ixz="0" iyy="0.305" iyz="0" izz="0.5445"/>
    </inertial>
    <visual name="wheel_fr_visual">
      <origin xyz="-0.0 0 0" rpy="-3.141593 0 0"/>
      <geometry>
        <cylinder radius="0.254" length="0.127"/>
      </geometry>
      <material name="Tire_001.006"/>
    </visual>
    <collision name="wheel_fr_collision">
      <origin xyz="0 0 0" rpy="-3.141593 0 0"/>
      <geometry>
        <cylinder radius="0.254" length="0.127"/>
      </geometry>
    </collision>
  </link>

  <link name="wheel_rl">
    <inertial>
      <origin xyz="0.00 0 0" rpy="0 0 0"/>
      <mass value="0.75"/>
      <inertia ixx="0.305" ixy="0" ixz="0" iyy="0.305" iyz="0" izz="0.5445"/>
    </inertial>
    <visual name="wheel_rl_visual">
      <origin xyz="-0.0 0 0" rpy="-3.141593 0 0"/>
      <geometry>
        <cylinder radius="0.254" length="0.127"/>
      </geometry>
      <material name="Tire_001.005"/>
    </visual>
    <collision name="wheel_rl_collision">
      <origin xyz="-0.00 0 0" rpy="-3.141593 0 0"/>
      <geometry>
      <cylinder radius="0.254" length="0.127"/>
      </geometry>
    </collision>
  </link>

  <link name="wheel_rr">
    <inertial>
      <origin xyz="0.00 0 0" rpy="0 0 0"/>
      <mass value="0.75"/>
      <inertia ixx="0.305" ixy="0" ixz="0" iyy="0.305" iyz="0" izz="0.5445"/>
    </inertial>
    <visual name="wheel_rr_visual">
      <origin xyz="-0.0 0 0" rpy="-3.141593 0 0"/>
      <geometry>
        <cylinder radius="0.254" length="0.127"/>
      </geometry>
      <material name="Tire_001.007"/>
    </visual>
    <collision name="wheel_rr_collision">
      <origin xyz="0.00 0 0" rpy="-3.141593 0 0"/>
      <geometry>
      <cylinder radius="0.254" length="0.127"/>
      </geometry>
    </collision>
  </link>

  <joint name="rearaxle" type="fixed">
    <origin xyz="-0.9668 0 0.0" rpy="0 0 0"/>
    <parent link="base_footprint"/>
    <child link="rearaxle"/>
  </joint>

  <joint name="frontaxle" type="fixed">
    <origin xyz="0.9668 0 0.0" rpy="0 0 0"/>
    <parent link="base_footprint"/>
    <child link="frontaxle"/>
  </joint>

  <joint name="base_link" type="fixed">
    <origin xyz="-0.15875 0 0.508" rpy="0 0 0"/>
    <parent link="base_footprint"/>
    <child link="base_link"/>
  </joint>

  <joint name="front_end" type="fixed">
    <origin xyz="1.0033 0 0.508" rpy="-1.570796 0 0"/>
    <parent link="base_footprint"/>
    <child link="front_end"/>
  </joint>

  <joint name="dome" type="fixed">
    <origin xyz="-0.1 0 1.258" rpy="-1.570796 0 0"/>
    <parent link="base_footprint"/>
    <child link="dome"/>
  </joint>

  <joint name="dome_small" type="fixed">
    <origin xyz="0.7 0 0.9" rpy="-1.570796 0 0"/>
    <parent link="base_footprint"/>
    <child link="dome_small"/>
  </joint>

  <joint name="trunk" type="fixed">
    <origin xyz="-1.05 0 1.1" rpy="0 -0.1 0.0"/>
    <parent link="base_footprint"/>
    <child link="trunk"/>
  </joint>

  <joint name="left_steer" type="revolute">
    <origin xyz="1.12555 0.6258 -0.254" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="left_steer"/>
    <axis xyz="0 0 1.0"/>
    <limit lower="-0.7" upper="0.7" effort="500000.0" velocity="100.0"/>
  </joint>

  <joint name="right_steer" type="revolute">
    <origin xyz="1.12555 -0.6258 -0.254" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="right_steer"/>
    <axis xyz="0 0 1.0"/>
    <limit lower="-0.7" upper="0.7" effort="500000.0" velocity="100.0"/>
  </joint>

  <joint name="wheel_fl" type="continuous">
    <origin xyz="0.0 0.0 0.0" rpy="-1.570796 0 0"/>
    <parent link="left_steer"/>
    <child link="wheel_fl"/>
    <axis xyz="0 0 1.0"/>
    <limit effort="5000.0" velocity="2.235"/>
  </joint>

  <joint name="wheel_fr" type="continuous">
    <origin xyz="0 0 0" rpy="-1.570796 0 0"/>
    <parent link="right_steer"/>
    <child link="wheel_fr"/>
    <axis xyz="0 0 1.0"/>
    <limit effort="5000.0" velocity="2.235"/>
  </joint>

  <joint name="wheel_rl" type="continuous">
    <origin xyz="-0.80805 0.6258 -0.254" rpy="-1.570796 0 0"/>
    <parent link="base_link"/>
    <child link="wheel_rl"/>
    <axis xyz="0 0 1.0"/>
    <limit effort="10000.0" velocity="2.235"/>
  </joint>

  <joint name="wheel_rr" type="continuous">
    <origin xyz="-0.80805 -0.6258 -0.254" rpy="-1.570796 0 0"/>
    <parent link="base_link"/>
    <child link="wheel_rr"/>
    <axis xyz="0 0 1.0"/>
    <limit effort="10000.0" velocity="2.235"/>
  </joint>

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="grey">
    <color rgba="0.5 0.5 0.5 1" />
  </material>
  <material name="black">
    <color rgba="0.2 0.2 0.2 1" />
  </material>

</robot>
